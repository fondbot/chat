{
    "name": "fondbot/chat",
    "description": "Interact with your bot using terminal.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Boldyrev",
            "email": "iboldurev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "fondbot/framework": "^3.0",
        "clue/stdio-react": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "FondBot\\Chat\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "FondBot\\Chat\\ChatServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
